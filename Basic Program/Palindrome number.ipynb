   "source": [
    "n=int(input(\"Enter any digit \"))\n",
    "temp=n\n",
    "reversed=0\n",
    "while n > 0:\n",
    "    rem = n % 10\n",
    "    reversed=reversed*10+rem\n",
    "    n=n//10\n",
    "if temp==reversed :\n",
    "    print(temp,\"is a palindrome number\")\n",
    "else:\n",
    "    print(temp,\"not a palindrome number\")"
   ]
