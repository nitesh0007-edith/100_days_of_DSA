    "def getEvenOdd(l):\n",
    "    even=[]\n",
    "    odd=[]\n",
    "    for i in l:\n",
    "        if i%2==0:\n",
    "            even.append(i)\n",
    "        else:\n",
    "            odd.append(i)\n",
    "    return even,odd\n",
    "\n",
    "print(\"Enter List:\")\n",
    "l=list(map(int,input().split(\" \")))\n",
    "even,odd=getEvenOdd(l)\n",
    "print(even)\n",
    "print(odd)"
   ]
